{
  "name": "vite-components-playground",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf lib && mkdir lib",
    "build": "vite build",
    "build:all": "npm run clean && npm run build:es",
    "build:es": "rollup --config build/rollup.config.es.js",
    "release": "standard-version && npm run cz:changelog",
    "cz:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "deploy": "bash deploy.sh"
  },
  "dependencies": {
    "vue": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@iconify/vue": "^2.0.0-rc.1",
    "@iconify/json": "^1.1.248",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@tailwindcss/typography": "^0.2.0",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "clean-css": "^4.2.3",
    "conventional-changelog-cli": "^2.0.34",
    "markdown-it": "^11.0.1",
    "markdown-it-prism": "^2.1.2",
    "postcss-nested": "^4.2.3",
    "rimraf": "^3.0.2",
    "rollup": "^1.16.7",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-vue": "^6.0.0-beta.4",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "standard-version": "^8.0.0",
    "tailwindcss": "^1.8.10",
    "vite": "^1.0.0-rc.1",
    "vite-plugin-vuedoc": "^0.0.9",
    "vue-router": "^4.0.0-beta.4"
  }
}
